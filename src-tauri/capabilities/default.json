{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default desktop capability",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "core:window:allow-toggle-maximize",
    "shell:allow-open",
    "dialog:allow-open",
    "fs:allow-read-text-file",
    "notification:default"
  ]
}
